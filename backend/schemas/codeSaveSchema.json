{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
      "lastEdited",
      "description",
      "url"
  ],
  "properties": {
      "lastEdited": {
          "type": "string",
          "default": "",
          "title": "The last edit Schema",
          "examples": [
              "4/22/22"
          ]
      },
      "description": {
          "type": "string",
          "default": "",
          "title": "The description Schema",
          "examples": [
              "blablabla"
          ]
      },
      "url": {
          "type": "string",
          "default": "",
          "title": "The url Schema",
          "examples": [
              "blablabla"
          ]
      }
  },
  "examples": [{
      "lastEdited": "4/22/22",
      "url": "https://quickchart.io/qr?text=Hello world&size=200",
      "description": "blablabla"
  }]
}